<?xml version="1.0" encoding="utf-8"?>

<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:gravity="center"
    android:layout_width="match_parent"
    android:layout_height="96dp"
    android:padding="31dp"
    android:orientation="horizontal"
    android:weightSum="1">
    <ImageView
        android:layout_width="50dp"
        android:layout_height="match_parent"
        android:src="@drawable/human"/>

    <TextView
        android:layout_marginStart="36dp"
        android:id="@+id/homeProtectingItem_textView_phoneNumber"
        android:gravity="center"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:text="보호 대상자 정보"
        android:textSize="21dp"
        android:fontFamily="@font/ps_regular"
        android:textColor="@color/toss_black_700"/>

    <TextView
        android:clickable="true"
        android:focusable="true"
        android:layout_marginStart="36dp"
        android:id="@+id/homeProtectorItem_btn_delete"
        android:gravity="center"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:text="해제"
        android:textSize="18dp"
        android:fontFamily="@font/ps_regular"
        android:textColor="@color/toss_black_200"/>
</LinearLayout>
