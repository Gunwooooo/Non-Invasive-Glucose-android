<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    tools:context=".home.HomeProtectingMenuActivity"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/toss_black_600">
    <com.factor.bouncy.BouncyNestedScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/toss_black_600"
        app:fling_animation_size=".8"
        app:overscroll_animation_size=".8">
        <LinearLayout
            android:background="@color/toss_black_600"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:padding="16dp">

            <LinearLayout
                android:focusable="true"
                android:clickable="true"
                android:id="@+id/homeProtectingMenu_btn_back"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">
                <ImageView
                    android:id="@+id/homeProtectingMenu_imageView_back"
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    app:tint="@color/white" />
                <TextView
                    android:layout_marginTop="3dp"
                    android:gravity="center"
                    android:layout_marginStart="5dp"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:text="Home"
                    android:textColor="@color/white"
                    android:textSize="18dp"
                    android:fontFamily="@font/ps_regular"/>
            </LinearLayout>

            <LinearLayout
                android:layout_marginTop="16dp"
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="match_parent">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">
                    <LinearLayout
                        android:background="@drawable/background_white_ring_shape"
                        android:padding="5dp"
                        android:layout_width="112dp"
                        android:layout_height="112dp"
                        android:orientation="horizontal">
                        <ImageView
                            android:id="@+id/homeProtectingMenu_imageView_profile"
                            android:layout_gravity="bottom"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent" />
                    </LinearLayout>

                    <LinearLayout
                        android:gravity="center"
                        android:layout_marginStart="16dp"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="vertical">
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal">

                            <TextView
                                android:id="@+id/homeProtectingMenu_textView_nickname"
                                android:layout_width="wrap_content"
                                android:layout_height="match_parent"
                                android:fontFamily="@font/ps_bold"
                                android:gravity="center_vertical"
                                android:text="         "
                                android:textColor="@color/white"
                                android:textSize="21dp" />

                            <LinearLayout
                                android:focusable="false"
                                android:clickable="false"
                                android:gravity="center"
                                android:layout_marginStart="16dp"
                                android:background="@drawable/background_layout_toss_black_100_radius_20"
                                android:layout_width="60dp"
                                android:layout_height="match_parent">
                                <TextView
                                    android:id="@+id/homeProtectingMenu_textView_age"
                                    android:gravity="center_vertical"
                                    android:layout_width="wrap_content"
                                    android:layout_height="match_parent"
                                    android:text="    세"
                                    android:textColor="@color/iphone_gray_500"
                                    android:fontFamily="@font/ps_regular"
                                    android:textSize="18dp"/>
                            </LinearLayout>
                        </LinearLayout>
                        <LinearLayout
                            android:gravity="bottom"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical">
                            <TextView
                                android:id="@+id/homeProtectingMenu_textView_sex"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text=""
                                android:textColor="@color/toss_black_150"
                                android:fontFamily="@font/ps_regular"
                                android:textSize="18dp"/>
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>

                <LinearLayout
                    android:layout_marginTop="24dp"
                    android:layout_width="match_parent"
                    android:layout_height="36dp"
                    android:background="@drawable/background_layout_android_blue_100_radius_20">
                    <LinearLayout
                        android:layout_margin="4dp"
                        android:clickable="true"
                        android:focusable="true"
                        android:background="@drawable/ripple_android_blue_100_radius_20"
                        android:id="@+id/homeProtectingMenu_layout_message"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">
                        <TextView
                            android:gravity="center"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:text="메시지"
                            android:textSize="18dp"
                            android:fontFamily="@font/ps_bold"
                            android:textColor="@color/toss_black_700"/>
                    </LinearLayout>
                </LinearLayout>

                <LinearLayout
                    android:layout_marginTop="24dp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:weightSum="1">
                    <LinearLayout
                        android:id="@+id/homeProtectingMenu_layout_thermometer"
                        android:clickable="true"
                        android:focusable="true"
                        android:layout_weight="0.5"
                        android:layout_height="wrap_content"
                        android:background="@drawable/background_layout_android_blue_100_radius_20"
                        android:layout_width="0dip">
                        <LinearLayout
                            android:layout_margin="4dp"
                            android:gravity="center"
                            android:padding="24dp"
                            android:background="@drawable/ripple_android_blue_100_radius_20"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical">

                            <ImageView
                                android:layout_width="64dp"
                                android:layout_height="64dp"
                                android:padding="3dp"
                                android:src="@drawable/icon_color_thermometer" />

                            <TextView
                                android:layout_marginTop="16dp"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="날짜별 체온"
                                android:fontFamily="@font/ps_bold"
                                android:textSize="18dp"
                                android:textColor="@color/toss_black_700"/>
                        </LinearLayout>
                    </LinearLayout>
                    <LinearLayout
                        android:id="@+id/homeProtectingMenu_layout_thermometerAnalysis"
                        android:layout_marginStart="16dp"
                        android:clickable="true"
                        android:focusable="true"
                        android:layout_weight="0.5"
                        android:layout_height="wrap_content"
                        android:background="@drawable/background_layout_android_blue_100_radius_20"
                        android:layout_width="0dip">
                        <LinearLayout
                            android:layout_margin="4dp"
                            android:gravity="center"
                            android:padding="24dp"
                            android:background="@drawable/ripple_android_blue_100_radius_20"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical">

                            <ImageView
                                android:layout_width="64dp"
                                android:layout_height="64dp"
                                android:padding="3dp"
                                android:src="@drawable/icon_color_thermometer_analysis"/>

                            <TextView
                                android:layout_marginTop="16dp"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="체온 분석"
                                android:fontFamily="@font/ps_bold"
                                android:textSize="18dp"
                                android:textColor="@color/toss_black_700"/>
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>

                <LinearLayout
                    android:layout_marginTop="16dp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:weightSum="1">
                    <LinearLayout
                        android:id="@+id/homeProtectingMenu_layout_heart"
                        android:clickable="true"
                        android:focusable="true"
                        android:layout_weight="0.5"
                        android:layout_height="wrap_content"
                        android:background="@drawable/background_layout_android_blue_100_radius_20"
                        android:layout_width="0dip">
                        <LinearLayout
                            android:layout_margin="4dp"
                            android:gravity="center"
                            android:padding="24dp"
                            android:background="@drawable/ripple_android_blue_100_radius_20"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical">

                            <ImageView
                                android:layout_width="64dp"
                                android:layout_height="64dp"
                                android:padding="3dp"
                                android:src="@drawable/icon_color_heart"/>

                            <TextView
                                android:layout_marginTop="16dp"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="날짜별 심박수"
                                android:fontFamily="@font/ps_bold"
                                android:textSize="18dp"
                                android:textColor="@color/toss_black_700"/>
                        </LinearLayout>
                    </LinearLayout>
                    <LinearLayout
                        android:id="@+id/homeProtectingMenu_layout_heartAnalysis"
                        android:layout_marginStart="16dp"
                        android:clickable="true"
                        android:focusable="true"
                        android:layout_weight="0.5"
                        android:layout_height="wrap_content"
                        android:background="@drawable/background_layout_android_blue_100_radius_20"
                        android:layout_width="0dip">
                        <LinearLayout
                            android:layout_margin="4dp"
                            android:gravity="center"
                            android:padding="24dp"
                            android:background="@drawable/ripple_android_blue_100_radius_20"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical">

                                <ImageView
                                    android:layout_width="64dp"
                                    android:layout_height="64dp"
                                    android:padding="3dp"
                                    android:src="@drawable/icon_color_heart_analysis" />

                            <TextView
                                android:layout_marginTop="16dp"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="심박수 분석"
                                android:fontFamily="@font/ps_bold"
                                android:textSize="18dp"
                                android:textColor="@color/toss_black_700"/>
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>

                <LinearLayout
                    android:layout_marginTop="16dp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:weightSum="1">
                    <LinearLayout
                        android:id="@+id/homeProtectingMenu_layout_glucose"
                        android:clickable="true"
                        android:focusable="true"
                        android:layout_weight="0.5"
                        android:layout_height="wrap_content"
                        android:background="@drawable/background_layout_android_blue_100_radius_20"
                        android:layout_width="0dip">
                        <LinearLayout
                            android:layout_margin="4dp"
                            android:gravity="center"
                            android:padding="24dp"
                            android:background="@drawable/ripple_android_blue_100_radius_20"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical">

                            <ImageView
                                android:layout_width="64dp"
                                android:layout_height="64dp"
                                android:padding="3dp"
                                android:src="@drawable/icon_color_glucose" />

                            <TextView
                                android:layout_marginTop="16dp"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="날짜별 혈당"
                                android:fontFamily="@font/ps_bold"
                                android:textSize="18dp"
                                android:textColor="@color/toss_black_700"/>
                        </LinearLayout>
                    </LinearLayout>
                    <LinearLayout
                        android:id="@+id/homeProtectingMenu_layout_glucoseAnalysis"
                        android:layout_marginStart="16dp"
                        android:clickable="true"
                        android:focusable="true"
                        android:layout_weight="0.5"
                        android:layout_height="wrap_content"
                        android:background="@drawable/background_layout_android_blue_100_radius_20"
                        android:layout_width="0dip">
                        <LinearLayout
                            android:layout_margin="4dp"
                            android:gravity="center"
                            android:padding="24dp"
                            android:background="@drawable/ripple_android_blue_100_radius_20"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical">

                            <ImageView
                                android:layout_width="64dp"
                                android:layout_height="64dp"
                                android:padding="3dp"
                                android:src="@drawable/icon_color_glucose_analysis" />

                            <TextView
                                android:layout_marginTop="16dp"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="혈당 분석"
                                android:fontFamily="@font/ps_bold"
                                android:textSize="18dp"
                                android:textColor="@color/toss_black_700"/>
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="60dp"/>
            </LinearLayout>
        </LinearLayout>
    </com.factor.bouncy.BouncyNestedScrollView>
</LinearLayout>